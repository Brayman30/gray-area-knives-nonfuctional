{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["eslint:recommended", "plugin:vue/vue3-recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["vue"],
  "rules": {},
  "overrides": [
    {
      "files": ["./pages/*"],
      "rules": {
        "vue/multi-word-component-names": "off"
      }
    }
  ]
}
